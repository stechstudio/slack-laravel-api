{
  "name": "stechstudio/slack-laravel-api",
  "description": "Slack SDK for writing our own Slack App integrations.",
  "type": "library",
  "require": {
    "ext-intl": "*",
    "guzzlehttp/psr7": "^1.8",
    "php-http/curl-client": "^2.2",
    "jolicode/slack-php-api": "^4.3",
    "ramsey/uuid": "^4.1",
    "guzzlehttp/guzzle": "^7.3"
  },
  "autoload": {
    "classmap": [],
    "psr-4": {
      "STS\\Slack\\": "src",
      "STS\\Slack\\Tests\\": "tests"
    },
    "files": []
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Bubba",
      "email": "rob@stechstudio.com"
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "STS\\Slack\\Providers\\SlackSdk"
      ]
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.89",
    "phpstan/phpstan-doctrine": "^0.12.39",
    "illuminate/http": "^8.46",
    "illuminate/support": "^8.46",
    "laravel/framework": "^8.46",
    "phpunit/phpunit": "^9.5"
  }
}
