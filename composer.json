{
  "name": "stechstudio/slack-laravel-api",
  "description": "Slack SDK for writing our own Slack App integrations.",
  "type": "library",
  "require": {
    "php": ">=7.2",
    "ext-intl": "*",
    "guzzlehttp/psr7": "^1.5",
    "php-http/curl-client": "^1.7",
    "jolicode/slack-php-api": "^1.0",
    "ramsey/uuid": "^3.8",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "STS\\Slack\\": "src",
      "STS\\Slack\\Tests\\": "tests"
    },
    "files": [
    ]
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Bubba",
      "email": "rob@stechstudio.com"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "phpstan/phpstan": "^0.11.2",
    "phpstan/phpstan-doctrine": "^0.11.1",
    "illuminate/http": "^5.7|^6.0",
    "illuminate/support": "^5.7|^6.0",
    "laravel/framework": "^5.7|^6.0",
    "phpunit/phpunit": "^8.3",
    "orchestra/testbench": "^4.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "STS\\Slack\\Providers\\SlackSdk"
      ]
    }
  }
}
